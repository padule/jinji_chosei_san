%h1='イベントページ'

%div
  %p='イベント名'
  =@event.name
  %p='説明'
  =@event.description

  %p='日程'
  %table
    %thead
      %tr
        %th='日程'
        -@event.members.each do |member|
          %th=member.name
    %tbody
    -@event.schedules.each do |schedule|
      %tr
        %td=schedule.date_time
        -@event.members.each do |member|
          -member.member_schedules.each do |member_schedule|
            member_schedule.enabled
